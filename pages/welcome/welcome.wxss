page {
    height: 100%;
    background-color: #b3d4db;
}
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
}


.user-name {
    margin-top: 100rpx;
    font-size: 32rpx;
    font-weight: bold;
}

.moto-wrapper {
    margin-top: 200rpx;
    border: 1rpx solid #405f80;
    width: 200rpx;
    height: 80rpx;
    text-align: center;
    border-radius: 5rpx;
}
.moto {
    font-size: 22rpx;
    font-weight: bold;
    line-height: 80rpx;
    color: #405f80;
}